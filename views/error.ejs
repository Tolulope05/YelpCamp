<% layout('layouts/boilerplate') %>
<div class="row">
    <div class="col-6 offset-3">
        <div class="alert alert-danger overflow-auto" role="alert">
            <h4 class="alert-heading"> <%= err.message %> </h4>
            <hr>
            <p>We encounter an error</p>
            <% if (process.env.NODE_ENV !== "production")  { %>
            <p class="mb-0"><%= err.stack %></p>
            <% } %>
        </div>
    </div>
</div>